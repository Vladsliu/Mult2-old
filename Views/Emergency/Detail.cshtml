@model Category


<div class="container">

    <h1 class="my-4">
        Pets
        <small>Profile information</small>
    </h1>

    <div class="row">

        <div class="col-md-8">
            <img class="img-fluid" src="@Model.Photo" alt="">
        </div>

        <div class="col-md-4">
            <h3 class="my-3">@Model.Name</h3>
            <p>@Model.Description</p>
            <h3 class="my-3">Detail</h3>
			<ul>
				<li>Name: @Model.Name</li>
                <li>CreatedDate: @Model.CreateDateTime</li>  
            </ul>
        </div>
    </div>
</div>
